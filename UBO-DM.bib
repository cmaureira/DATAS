isbn = {0387291466},
title = {{Similarity Search: The Metric Space Approach (Advances in Database Systems)}},
url = {http://www.amazon.ca/exec/obidos/redirect?tag=citeulike09-20{\&}path=ASIN/0387291466{\%}5Cnhttp://www.amazon.de/exec/obidos/redirect?tag=citeulike01-21{\&}path=ASIN/0387291466{\%}5Cnhttp://www.amazon.fr/exec/obidos/redirect?tag=citeulike06-21{\&}path=ASIN/03},
